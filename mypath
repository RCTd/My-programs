#include <iostream>
using namespace std;

int main()
{
    int i,j,m[21][21],sx,sy;
    cin>>sx>>sy;
    for(j=0;j<=20;j++)for(i=0;i<=20;i++)m[i][j]=0;
    m[sx-1][sy-1]=1;
    for(j=0;j<=20;j++)
        for(i=0;i<=20;i++)
        {
            if(m[i][j]==0)
            {
                if(j-1<0){if(m[i][20]>0) m[i][j]=m[i][20]+1;}else
                    if(m[i][j-1]>0) m[i][j]=m[i][j-1]+1;
                if(i+1>20){if(m[0][j]>0) m[i][j]=m[0][j]+1;}else
                    if(m[i+1][j]>0) m[i][j]=m[i+1][j]+1;
                if(j+1>20){if(m[i][0]>0)m[i][j]=m[i][0]+1;}else
                    if(m[i][j+1]>0)m[i][j]=m[i][j+1]+1;
                if(i-1<0){if(m[20][j]>0)m[i][j]=m[20][j]+1;}else
                    if(m[i-1][j]>0)m[i][j]=m[i-1][j]+1;
            }
            if(j-1<0){if(m[i][20]<m[i][j]&&m[i][20]>0)m[i][j]=m[i][20]+1;}else
                if(m[i][j-1]<m[i][j]&&m[i][j-1]>0)m[i][j]=m[i][j-1]+1;
            if(i+1>20){if(m[0][j]<m[i][j]&&m[0][j]>0)m[i][j]=m[0][j]+1;}else
                if(m[i+1][j]<m[i][j]&&m[i+1][j]>0)m[i][j]=m[i+1][j]+1;
            if(j+1>20){if(m[i][0]<m[i][j]&&m[i][0]>0)m[i][j]=m[i][0]+1;}else
                if(m[i][j+1]<m[i][j]&&m[i][j+1]>0)m[i][j]=m[i][j+1]+1;
            if(i-1<0){if(m[20][j]<m[i][j]&&m[20][j]>0)m[i][j]=m[20][j]+1;}else
                if(m[i-1][j]<m[i][j]&&m[i-1][j]>0)m[i][j]=m[i-1][j]+1;
        }
    cout<<"\n";
    for(j=0;j<21;j++)
    {
        for(i=0;i<21;i++)
        {
            if(m[i][j]<9)cout<<m[i][j]<<"  ";else
            cout<<m[i][j]<<" ";
        }cout<<"\n";
    }
    return 0;
}
