#include <iostream>
#include <vector>
#include <windows.h>
using namespace std;
void da();
int p[8];
int y=0,x=0;
bool ex=false;
int t[8][8];
int c,i,j,a;
int main()
{
    for(int i=0;i<8;i++)p[i]=11;
    while((x<8)||(p[0]<8))
    {
        ex=false;
        while(ex==false)
        {
            if(p[x]==8){ex=true;y--;p[x]--;}
            system("CLS");
            for(i=0; i<8; i++){for(int j=0; j<8; j++)if(p[j]==i)cout<<1;else cout<<0;cout<<'\n';}
            p[x]=y;
            da();
            for(i=0; i<8; i++)for(int j=0; j<8; j++)t[i][j]=0;
            for(a=0; a<=x&&ex==false; a++)
            {
                t[a][p[a]]=1;
                for(i=0; i<8&&ex==false; i++)
                {
                    for(c=0; c<8&&ex==false; c++)if((p[c]==p[a])&&(c==i)&&(c!=a))ex=true;
                    t[i][p[a]]=1;
                }
                for(i=a,j=p[a]; i<8&&j<8&&ex==false; i++,j++)
                {
                    for(c=0; c<8&&ex==false; c++)if((p[c]==j)&&(c==i)&&(c!=a))ex=true;
                    t[i][j]=1;
                }
                for(int i=a,j=p[a]; i>=0&&j<8&&ex==false; i--,j++)
                {
                    for(c=0; c<8&&ex==false; c++)if((p[c]==j)&&(c==i)&&(c!=a))ex=true;
                    t[i][j]=1;
                }
            }
            if(ex==false)
            {
                y=0;
                x++;
            }
        }
        y++;
        p[x]++;
        if(y==8)
        {
            p[x]=11;
            x--;
            y=p[x]+1;
            p[x]++;
        }
    }
    if(p[0]==8)
        cout<<"Nu se poate";
    else
        for(int i=0; i<8; i++)
            cout<<'('<<i<<' '<<p[i]<<') ';
    return 0;
}
void da()
{

}
