#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int n,x,m,r,p,i,t;
    cin>>x;
    vector<int> v;
    while(x)
    {
        v.push_back(x);
        cin>>x;
    }
    n=v.size();
    while(n)
    {
        for(r=n,i=1,p=0,m=v[0];i<r;i++)
        {
            if(m<v[i])
            {
                m=v[i];
                p=i;
            }
        }
        n--;
        t=v[n];
        v[n]=v[p];
        v[p]=t;
    }
    for(i=0;i<v.size();i++)
        cout<<v[i]<<" ";
    return 0;
}
